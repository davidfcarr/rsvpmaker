{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "rsvpmaker/excerpt",
  "title": "RSVPMaker Excerpt",
  "icon": "calendar-alt",
  "category": "rsvpmaker",
  "description": "Simple excerpt of an RSVPMaker event",
  "keywords": [
    "RSVPMaker",
    "Events",
    "Excerpt"
  ],
  "attributes": {
    "hide_type": {
      "type": "boolean",
      "default": false
    },
    "max": {
      "type": "integer",
      "default": 55
    }
  },
  "usesContext": [
    "postId"
  ],
  "version": "1",
  "supports": {
    "html": false
  },
  "textdomain": "rsvpmaker",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php",
  "style": "file:./style-index.css"
}
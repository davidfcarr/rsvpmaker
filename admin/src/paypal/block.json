            "august": {
            "type": "string",
            "default": ""
            },
            "september": {
            "type": "string",
            "default": ""
            },
            "october": {
            "type": "string",
            "default": ""
            },
            "november": {
            "type": "string",
            "default": ""
            },
            "december": {
            "type": "string",
            "default": ""
            }
  },
  "version": "2",
  "textdomain": "rsvpmaker",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php",
  "style": "file:./style-index.css"

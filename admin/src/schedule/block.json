{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rsvpmaker/schedule",
	"title": "RSVPMaker Schedule",
    "icon": "admin-comments",
    "description": "Daily schedule of events",
	"category": "rsvpmaker",
	"keywords": [
		"RSVPMaker",
		"Event",
		"Schedule"
	],
"attributes": {
	"start": {
		"type": "string",
		"default": ""
	},
	"start_on": {
		"type": "string",
		"default": "0"
	},
	"end": {
		"type": "string",
		"default": ""
	},
	"end_on": {
		"type": "string",
		"default": "0"
	},
	"type": {
		"type": "string",
		"default": "0"
	},
	"convert_tz": {
		"type": "boolean",
		"default": false
	},
	"limit": {
		"type": "integer",
		"default": 30
	}
},
  "version": "2",
  "textdomain": "rsvpmaker",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php",
  "style": "file:./style-index.css"
}
